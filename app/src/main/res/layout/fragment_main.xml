<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context=".MainActivity">

    <TextView
        android:id="@+id/register_location_label"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:text="GNSS Register"
        android:textStyle="bold"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintBottom_creator="1"
        app:layout_constraintBottom_toTopOf="@+id/register_location"
        app:layout_constraintRight_toRightOf="@+id/register_location"
        tools:layout_constraintLeft_creator="1"
        android:layout_marginBottom="15dp"
        app:layout_constraintLeft_toLeftOf="@+id/register_location"
        app:layout_constraintHorizontal_bias="0.0" />

    <Switch
        android:id="@+id/register_location"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:singleLine="true"
        android:text="@string/location_label"
        tools:text="Register"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintBottom_creator="1"
        app:layout_constraintBottom_toTopOf="@+id/checkBox"
        app:layout_constraintRight_toRightOf="@+id/checkBox"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintLeft_toLeftOf="@+id/checkBox"
        app:layout_constraintHorizontal_bias="0.0"
        android:layout_marginBottom="19dp" />

    <CheckBox
        android:id="@+id/checkBox"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:text="Carrier Phase"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintBottom_creator="1"
        app:layout_constraintBottom_toTopOf="@+id/register_sensor"
        app:layout_constraintRight_toRightOf="@+id/register_sensor"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintLeft_toLeftOf="@+id/register_sensor"
        app:layout_constraintHorizontal_bias="0.0"
        android:layout_marginBottom="49dp" />

    <Switch
        android:id="@+id/register_sensor"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:text="Android Sensor"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintBottom_creator="1"
        app:layout_constraintBottom_toTopOf="@+id/textView"
        app:layout_constraintRight_toRightOf="@+id/textView"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintLeft_toLeftOf="@+id/textView"
        app:layout_constraintHorizontal_bias="0.0"
        android:layout_marginBottom="8dp" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:text="TextView"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintBottom_creator="1"
        app:layout_constraintBottom_toTopOf="@+id/sw_info"
        app:layout_constraintRight_toRightOf="@+id/sw_info"
        tools:layout_constraintLeft_creator="1"
        android:layout_marginBottom="164dp"
        app:layout_constraintLeft_toLeftOf="@+id/sw_info"
        app:layout_constraintHorizontal_bias="0.0" />

    <Button
        android:id="@+id/rstbutton"
        android:layout_width="180dp"
        android:layout_height="wrap_content"
        android:text="Rset GNSSClock"
        tools:layout_constraintTop_creator="1"
        android:layout_marginStart="8dp"
        tools:layout_constraintLeft_creator="1"
        android:layout_marginTop="31dp"
        app:layout_constraintTop_toBottomOf="@+id/textView"
        android:layout_marginLeft="27dp"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginBottom="8dp"
        app:layout_constraintBottom_toTopOf="@+id/exit"
        app:layout_constraintVertical_bias="1.0"
        android:layout_marginRight="8dp"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintHorizontal_bias="0.448" />

    <Button
        android:id="@+id/exit"
        android:layout_width="179dp"
        android:layout_height="wrap_content"
        android:singleLine="true"
        android:text="@string/exit"
        tools:layout_constraintBottom_creator="1"
        android:layout_marginStart="8dp"
        tools:layout_constraintLeft_creator="1"
        android:layout_marginLeft="27dp"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginRight="8dp"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintHorizontal_bias="0.451"
        android:layout_marginBottom="24dp"
        app:layout_constraintBottom_toTopOf="@+id/textView2" />

    <TextView
        android:id="@+id/sw_info"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:textStyle="bold"
        android:gravity="bottom"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintBottom_creator="1"
        android:layout_marginStart="8dp"
        app:layout_constraintBottom_toBottomOf="parent"
        android:layout_marginEnd="8dp"
        app:layout_constraintRight_toRightOf="parent"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintLeft_toLeftOf="parent" />

    <TextView
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:gravity="end|bottom"
        android:textStyle="italic|bold"
        android:text="@string/app_version"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintRight_creator="1"
        android:layout_marginStart="8dp"
        android:layout_marginEnd="8dp"
        app:layout_constraintRight_toRightOf="parent"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginBottom="8dp"
        android:id="@+id/textView2"
        app:layout_constraintBottom_toBottomOf="@+id/sw_info" />

    <ImageView
        android:id="@+id/imageView"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintLeft_toLeftOf="@+id/register_location_label"
        app:layout_constraintRight_toRightOf="@+id/register_location_label"
        app:srcCompat="@mipmap/kubolab"
        tools:layout_constraintBottom_creator="1"
        tools:layout_constraintLeft_creator="1"
        tools:layout_constraintRight_creator="1"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginTop="8dp"
        android:layout_marginBottom="8dp"
        app:layout_constraintBottom_toTopOf="@+id/register_location_label"
        app:layout_constraintVertical_bias="0.882" />

    <CheckBox
        android:id="@+id/useGLO"
        android:layout_width="109dp"
        android:layout_height="29dp"
        android:text="GLONASS"
        android:layout_marginRight="59dp"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginTop="8dp"
        app:layout_constraintTop_toBottomOf="@+id/checkBox"
        android:layout_marginBottom="8dp"
        app:layout_constraintBottom_toTopOf="@+id/register_sensor"
        app:layout_constraintVertical_bias="1.0" />

    <CheckBox
        android:id="@+id/useQZS"
        android:layout_width="88dp"
        android:layout_height="31dp"
        android:text="QZSS"
        android:layout_marginLeft="8dp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toLeftOf="@+id/useGLO"
        android:layout_marginRight="8dp"
        android:layout_marginTop="8dp"
        app:layout_constraintTop_toBottomOf="@+id/checkBox"
        android:layout_marginBottom="8dp"
        app:layout_constraintBottom_toTopOf="@+id/register_sensor" />

</android.support.constraint.ConstraintLayout>